{
  "name": "xobot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "rm -rf dist && tsc",
    "prestart": "npm run build",
    "start": "node dist/src/index.js",
    "cli": "npm run build && node dist/src/cli.js",
    "web": "npm run build && node dist/src/server.js",
    "dev:cli": "ts-node src/cli.ts",
    "dev:web": "ts-node src/server.ts",
    "pm2:start": "npm run build && pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs bsc-analyzer-web",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },  
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^17.2.3",
    "grammy": "^1.39.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
